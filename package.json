{
  "name": "training-app-angular",
  "version": "1.1.1",
  "description": "training-app-angular: An Ionic project",
  "dependencies": {
    "gulp": "^3.5.6",
    "gulp-sass": "^2.0.4",
    "gulp-concat": "^2.2.0",
    "gulp-minify-css": "^0.3.0",
    "gulp-rename": "^1.2.0"
  },
  "devDependencies": {
    "bower": "^1.3.3",
    "gulp-util": "^2.2.14",
    "shelljs": "^0.3.0"
  },
  "cordovaPlugins": [
    "cordova-plugin-device",
    "cordova-plugin-console",
    "cordova-plugin-whitelist",
    "cordova-plugin-splashscreen",
    "cordova-plugin-statusbar",
    "ionic-plugin-keyboard",
    "cordova-plugin-inappbrowser",
    {
      "id": "cordova-plugin-facebook4",
      "locator": "cordova-plugin-facebook4",
      "variables": {
        "APP_ID": "265471787126357",
        "APP_NAME": "com.kinvey.training"
      }
    },
    {
      "id": "phonegap-plugin-push",
      "locator": "phonegap-plugin-push",
      "variables": {
        "SENDER_ID": "856811466642"
      }
    },
    "cordova-plugin-dialogs",
    "cordova-plugin-vibration"
  ],
  "cordovaPlatforms": [
    "android",
    {
      "platform": "ios",
      "version": "4.1.0",
      "locator": "ios@4.1.0"
    }
  ],
  "scripts": {
    "emulate-android": "gulp && cordova emulate android",
    "emulate-ios": "gulp && cordova emulate ios",
    "run-android": "gulp && cordova run android",
    "run-ios": "gulp && cordova run ios",
    "install":"ionic state reset && bower install"
  }
}